{
  "useConfigFiles": true,
  "router": true,
  "routerHistoryMode": true,
  "vuex": true,
  "cssPreprocessor": "dart-sass",
  "plugins": {
    "@vue/cli-plugin-babel": {},
    "@vue/cli-plugin-eslint": {
      "config": "airbnb",
      "lintOn": [
        "save"
      ]
    },
    "@vue/cli-plugin-unit-jest": {},
    "vue-cli-plugin-storybook": {
      "version": "^2.0.0"
    },
    "vue-cli-plugin-vuetify": {
      "version": "latest",
      "preset": "configure",
      "replaceComponents": true,
      "iconFont": "md",
      "installFonts": true,
      "locale": "en",
      "useAlaCarte": true,
      "useCustomProperties": true,
      "usePolyfill": false,
      "useTheme": true
    },
    "vue-cli-plugin-thrivehive": {
      "version": "latest"
    }
  }
}
